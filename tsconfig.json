{
    "compileOnSave": true,
    "compilerOptions": {
        "module": "none",
        "sourceMap": true,
        "target": "es6",
        "declaration": true,
        "outDir": "./dist",
        "strict": true,
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "include": [
        "**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}